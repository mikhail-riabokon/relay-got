{
  "name": "relay-got",
  "version": "1.0.1",
  "description": "Relay example in Game of Thrones subject",
  "main": "index.js",
  "scripts": {
    "update-schema": "babel-node ./scripts/updateSchema.js",
    "webpack": "babel-node ./scripts/webpack/index.js",
    "dev:server": "nodemon --exec babel-node ./index.js",
    "dev:client": "HOT=1 NODE_ENV=development npm run webpack",
    "dev": "npm run update-schema && HOT=1 NODE_ENV=development npm run dev:server",
    "start": "NODE_ENV=production node ./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikhail-riabokon/relay-got.git"
  },
  "keywords": [
    "JavasScript",
    "ES6",
    "Relay",
    "GraphQL",
    "NodeJS",
    "express"
  ],
  "author": "Mikhail Riabokon",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/mikhail-riabokon/relay-got/issues"
  },
  "homepage": "https://github.com/mikhail-riabokon/relay-got#readme",
  "dependencies": {
    "express": "^4.13.4",
    "express-graphql": "^0.4.13",
    "graphql": "^0.4.18",
    "graphql-relay": "^0.3.6",
    "immutable": "^3.7.5",
    "less": "^2.5.3",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-modal": "^0.6.1",
    "react-relay": "^0.7.3"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-relay-plugin": "^0.7.3",
    "clean-webpack-plugin": "^0.1.8",
    "css-loader": "^0.23.1",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "file-loader": "^0.8.5",
    "less-loader": "^2.2.3",
    "nodemon": "^1.9.1",
    "react-transform-hmr": "^1.0.4",
    "style-loader": "^0.13.1",
